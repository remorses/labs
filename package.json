{
  "name": "@sterblue/labs",
  "private": true,
  "workspaces": [
    "packages/*/*",
    "!packages/example/app-electron-react",
    "!packages/example/app-react-native"
  ],
  "scripts": {
    "one:clean": "rm -rf \"$INIT_CWD/lib\" || echo ok ; rm -rf \"$INIT_CWD/dist\" || echo ok ; rm -rf \"$INIT_CWD/bin\" || echo ok ; echo cleaned",
    "one:echo": "echo $INIT_CWD",
    "one:env": "env PWD",
    "one:env2": "echo $(env PWD)",
    "one:babel": "babel --extensions .tsx,.ts --out-dir \"$INIT_CWD/lib\" \"$INIT_CWD/src\" || echo ok",
    "one:webpack": "cd \"$INIT_CWD\" && webpack --config \"$INIT_CWD/webpack.config.js\" || echo ok",
    "one:eslint": "eslint --ext .ts,.tsx --format stylish --no-color \"$INIT_CWD/src\"",
    "one:test": "jest \"$INIT_CWD\"",
    "all:clean": "rm -rf ./packages/*/*/lib || echo ok ; rm -rf ./packages/*/*/dist || echo ok ; rm -rf ./packages/*/*/bin || echo ok ; echo cleaned",
    "all:clean2": "rm -rf ./packages/*/*/lib || echo ok",
    "all:echo": "yarn workspaces foreach --all --topological-dev --parallel --jobs ${JOB_CONCURRENCY:-8} exec echo $(pwd) $PWD $(env PWD)",
    "all:env": "yarn workspaces foreach --all --topological-dev --parallel --jobs ${JOB_CONCURRENCY:-8} exec env PWD",
    "all:env2": "yarn workspaces foreach --all --topological-dev --parallel --jobs ${JOB_CONCURRENCY:-8} exec echo $(env PWD)",
    "all:babel": "yarn workspaces foreach --all --topological-dev --parallel --jobs ${JOB_CONCURRENCY:-8} exec yarn one:babel"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-export-default-from": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@babel/register": "7.11.5",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "babel-jest": "26.5.0",
    "babel-loader": "8.1.0",
    "core-js": "3.6.5",
    "dotenv": "8.2.0",
    "eslint": "7.10.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.0.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.5.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-environment-node": "26.5.0",
    "jest-pnp-resolver": "1.2.2",
    "jest-watch-typeahead": "0.6.1",
    "nodemon-webpack-plugin": "4.3.2",
    "pnp-webpack-plugin": "1.6.4",
    "prettier": "2.1.2",
    "react-app-polyfill": "1.0.6",
    "ts-loader": "8.0.3",
    "typescript": "4.0.3",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "2.5.2"
  },
  "resolutions": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@emotion/styled-base": "10.0.28",
    "@types/graphql": "14.2.0",
    "graphql": "14.6.0",
    "mmmagic": "0.5.3",
    "fsevents": "2.1.2",
    "sharp": "0.21.3"
  },
  "engines": {
    "node": ">=10.18.1",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@types/jest": "26.0.14",
    "@yarnpkg/core": "2.3.0",
    "@yarnpkg/pnpify": "2.3.1"
  }
}
